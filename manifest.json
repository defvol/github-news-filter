{
  "manifest_version": 2,
  "name": "GitHub news filter",
  "version": "0.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {},
  "content_scripts": [
    {
      "css": [
        "content.css"
      ],
      "js": [
        "content.js"
      ],
      "matches": [
        "*://github.com/*"
      ]
    }
  ]
}
