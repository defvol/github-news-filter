{
  "manifest_version": 2,
  "name": "GitHub news filter",
  "version": "0.4",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "css": [
        "content.css"
      ],
      "js": [
        "content.js"
      ],
      "matches": [
        "*://github.com/*"
      ]
    }
  ]
}
